{
    "compilerOptions": {
        "module": "ESNext",
        "moduleResolution": "Node",
        "resolveJsonModule": true,
        "useDefineForClassFields": true,
        "jsx": "preserve",
        "noImplicitThis": true,
        "strict": true,
        "target": "ESNext",
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
      }
  }